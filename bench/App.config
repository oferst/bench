<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>    
    <!-- cpbm is the program that generates the scatter/cactus graphs-->
    <add key="cpbm" value="c:\temp\cpbm-0.5\" />
    <add key="log" value="C:\temp\log.txt"/>
    <!-- history file is searched in the same directory as the executable. For a different benchmarking project, change this file-->
    <add key="history_filename" value="history.txt"/>
    
    <!-- info for running jobs remotely with PBS (submitting jobs via a remote qsub command). Runs via ssh, hence needs a private-key file.-->
    <add key="remote_user" value="ofers"/>
    <add key="remote_domain" value="tamnun.technion.ac.il"/>    
    <add key="remote_bench_dir" value="~/hmuc/test"/>
    <add key="remote_app_dir" value="~/hmuc"/>
    <add key="remote_script" value="hmuc.sh"/>

    <!-- here add the 3-letter string that prefixes the data to be logged from the output of the checked application.-->
    <add key="stat_tag" value="###"/>
    <!-- abort reading output if the following tag appears after stat_tag, e.g. ### Abort, and print Warning. Useful for aborting when the parameters are illegal. -->
    <add key="abort_tag" value="Abort"/>
    
  </appSettings>
</configuration>